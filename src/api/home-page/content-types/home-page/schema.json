    },
    "servicesPreview": {
      "displayName": "ServicesPreview",
      "type": "component",
      "repeatable": false,
      "component": "layout.services-preview"
    },
    "featuredEvent": {
      "displayName": "featuredEvent",
      "type": "component",
      "repeatable": true,
      "component": "layout.featured-event"
    },
    "articleSelection": {
      "displayName": "articleSelection",
      "type": "component",
      "repeatable": false,
      "component": "articles.article-selection"
    },
    "dynamicHomeSection": {
      "type": "dynamiczone",
      "components": [
        "layout.newsletter-form",
        "layout.latest-news"
      ]
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-information"
    }
